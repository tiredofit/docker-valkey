#!/command/with-contenv bash

DATA_PATH=${DATA_PATH:-"/data/db/"}
ENABLE_LOGS=${ENABLE_LOGS:-"TRUE"}
ENABLE_PERSISTENCE=${ENABLE_PERSISTENCE:-"TRUE"}
ENABLE_SOCKET=${ENABLE_SOCKET:-"TRUE"}
LOG_FILE=${LOG_FILE:-"valkey.log"}
LOG_LEVEL=${LOG_LEVEL:-"notice"}
LOG_PATH=${LOG_PATH:-"/logs/"}
VALKEY_PASS=${VALKEY_PASS:-}
VALKEY_PORT=${VALKEY_PORT:-"6379"}
REDIS_PASS=${VALKEY_PASS:-"${VALKEY_PASS}"}
REDIS_PORT=${VALKEY_PORT:-"${VALKEY_PORT}"}
SOCKET_FILE=${SOCKET_FILE:-"valkey.sock"}
SOCKET_PATH=${SOCKET_PATH:-"/var/run/valkey/"}
SOCKET_PERMISSIONS=${SOCKET_PERMISSIONS:-"777"}